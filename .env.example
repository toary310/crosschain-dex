# WalletConnect Project ID
# Get your project ID from https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# RPC URLs (optional - wagmi uses public RPCs by default)
NEXT_PUBLIC_ETHEREUM_RPC_URL=
NEXT_PUBLIC_POLYGON_RPC_URL=
NEXT_PUBLIC_ARBITRUM_RPC_URL=
NEXT_PUBLIC_OPTIMISM_RPC_URL=

# Alchemy API Key (for enhanced RPC endpoints)
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_api_key_here

# API Keys for price feeds and DEX aggregators
NEXT_PUBLIC_COINGECKO_API_KEY=
NEXT_PUBLIC_1INCH_API_KEY=
NEXT_PUBLIC_0X_API_KEY=

# Analytics and monitoring
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=
NEXT_PUBLIC_MIXPANEL_TOKEN=

# Error reporting and monitoring endpoints
NEXT_PUBLIC_ERROR_REPORTING_ENDPOINT=
NEXT_PUBLIC_SECURITY_ENDPOINT=
NEXT_PUBLIC_PERFORMANCE_WEBHOOK=
NEXT_PUBLIC_MONITORING_WEBHOOK=

# Environment
NODE_ENV=development
NEXT_PUBLIC_ENVIRONMENT=development
